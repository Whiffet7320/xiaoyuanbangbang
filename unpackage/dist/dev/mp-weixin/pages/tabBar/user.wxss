@charset "UTF-8";
/**
 * 下方引入的为uView UI的集成样式文件，为scss预处理器，其中包含了一些"u-"开头的自定义变量
 * 使用的时候，请将下面的一行复制到您的uniapp项目根目录的uni.scss中即可
 * uView自定义的css类名和scss变量，均以"u-"开头，不会造成冲突，请放心使用 
 */
page {
  height: 100%;
  background: #f7f8fa;
}
.user {
  width: 100%;
  height: 100%;
}
.user .Header {
  width: 100%;
  display: flex;
  flex-direction: column;
}
.user .Header .TopImg {
  position: absolute;
  z-index: -1;
}
.user .Header .TopImg image {
  width: 750rpx;
  height: 350rpx;
}
.user .Header .Navtop {
  display: flex;
  align-items: center;
  justify-content: center;
  height: 176rpx;
}
.user .Header .Navtop .Title {
  margin-top: 88rpx;
  font-size: 34rpx;
  font-weight: 700;
  color: #ffffff;
}
.user .Header .userInfo {
  height: 184rpx;
  display: flex;
  align-items: center;
  justify-content: space-between;
  margin: 40rpx 50rpx 32rpx 32rpx;
}
.user .Header .userInfo .avaUrl {
  display: flex;
  align-items: center;
}
.user .Header .userInfo .avaUrl image {
  width: 184rpx;
  height: 184rpx;
  border-radius: 50%;
  background-color: #eee;
}
.user .Header .userInfo .subject {
  width: 464rpx;
  height: 184rpx;
  display: flex;
  justify-content: space-between;
}
.user .Header .userInfo .subject .Content {
  display: flex;
  flex-direction: column;
  justify-content: center;
}
.user .Header .userInfo .subject .Content .Name {
  font-size: 44rpx;
  font-weight: 700;
  color: #ffffff;
}
.user .Header .userInfo .subject .Content .userPhone {
  font-size: 28rpx;
  font-weight: 400;
  color: #ffffff;
}
.user .Header .userInfo .subject .InfoTips {
  display: flex;
}
.user .Header .userInfo .subject .InfoTips .Set_up {
  display: flex;
}
.user .Header .userInfo .subject .InfoTips .Set_up image {
  width: 42rpx;
  height: 40rpx;
}
.user .Header .userInfo .subject .InfoTips .Notice {
  display: flex;
  margin-left: 46rpx;
}
.user .Header .userInfo .subject .InfoTips .Notice image {
  width: 40rpx;
  height: 40rpx;
}
.user .ZiChan {
  width: 702rpx;
  height: 220rpx;
  margin: 0 auto;
  background: #ffffff;
  border-radius: 10rpx;
  display: flex;
  flex-direction: column;
}
.user .ZiChan .Title {
  margin: 28rpx 0 0 34rpx;
  font-size: 28rpx;
  font-weight: 700;
  color: #000000;
}
.user .ZiChan .BanList {
  width: 100%;
  display: flex;
  align-items: center;
  justify-content: space-evenly;
  margin: 25rpx auto 0;
}
.user .ZiChan .BanList .StausList {
  display: flex;
  flex-direction: column;
  align-items: center;
}
.user .ZiChan .BanList .StausList .OneSele {
  display: flex;
  align-items: center;
}
.user .ZiChan .BanList .StausList .OneSele .Pic {
  display: flex;
  align-items: center;
}
.user .ZiChan .BanList .StausList .OneSele .Pic image {
  width: 52rpx;
  height: 54rpx;
}
.user .ZiChan .BanList .StausList .OneSele .Name {
  margin-left: 16rpx;
  font-size: 24rpx;
  font-weight: 700;
  color: #000000;
}
.user .ZiChan .BanList .StausList .TwoSele {
  margin-top: 16rpx;
  font-size: 24rpx;
  font-weight: 500;
  color: #000000;
}
.user .MyFunc {
  width: 702rpx;
  height: 220rpx;
  margin: 20rpx auto;
  background: #ffffff;
  border-radius: 10rpx;
  display: flex;
  flex-direction: column;
}
.user .MyFunc .Title {
  margin: 28rpx 0 0 34rpx;
  font-size: 28rpx;
  font-weight: 700;
  color: #000000;
}
.user .MyFunc .subject {
  width: 100%;
  display: flex;
  align-items: center;
  margin: 25rpx auto 0;
}
.user .MyFunc .subject .FuncList {
  display: flex;
  align-items: center;
  flex-direction: column;
  margin-left: 72rpx;
}
.user .MyFunc .subject .FuncList .Pic {
  display: flex;
  align-items: center;
}
.user .MyFunc .subject .FuncList .Pic image {
  width: 44rpx;
  height: 54rpx;
}
.user .MyFunc .subject .FuncList .Name {
  margin-top: 12rpx;
  font-size: 24rpx;
  font-weight: 500;
  color: #000000;
}
.user .OtherFunc {
  width: 702rpx;
  height: 332rpx;
  margin: 0 auto 88rpx;
  background: #ffffff;
  border-radius: 10rpx;
  display: flex;
  flex-direction: column;
}
.user .OtherFunc .Title {
  margin: 28rpx 0 0 34rpx;
  font-size: 28rpx;
  font-weight: 700;
  color: #000000;
}
.user .OtherFunc .subject {
  width: 660rpx;
  margin: 36rpx 54rpx;
  display: flex;
  align-items: center;
  flex-wrap: wrap;
}
.user .OtherFunc .subject .FuncList {
  margin-right: 68rpx;
  margin-bottom: 32rpx;
  display: flex;
  align-items: center;
  flex-direction: column;
}
.user .OtherFunc .subject .FuncList .Pic {
  display: flex;
  align-items: center;
  justify-content: center;
}
.user .OtherFunc .subject .FuncList .Pic image {
  width: 38rpx;
  height: 42rpx;
}
.user .OtherFunc .subject .FuncList .Name {
  margin-top: 12rpx;
  font-size: 24rpx;
  font-weight: 500;
  color: #000000;
}
.user .ExitLogin {
  width: 522rpx;
  height: 80rpx;
  margin: 0 auto 91rpx;
  background: #fe694f;
  border-radius: 10rpx;
  display: flex;
  align-items: center;
  justify-content: center;
  font-size: 28rpx;
  font-weight: 500;
  color: #ffffff;
}
.user .Mask {
  position: fixed;
  z-index: 1;
  top: 0;
  bottom: 0;
  left: 0;
  right: 0;
  height: 100%;
  width: 100%;
  background-color: #000;
  opacity: .3;
}
.user .Tips {
  position: fixed;
  z-index: 2;
  bottom: 0;
  display: flex;
  align-items: center;
  flex-direction: column;
  justify-content: space-evenly;
  width: 100%;
  height: 434rpx;
  background: #ffffff;
  border-radius: 40rpx 40rpx 0rpx 0rpx;
}
.user .Tips .Tes {
  font-size: 28rpx;
  font-weight: 700;
  color: #ffa900;
}
.user .Tips .Cont {
  font-size: 28rpx;
  font-weight: 500;
  color: #000000;
}
.user .Tips .NoChak {
  width: 100%;
  border-top: 8rpx solid #f7f8fa;
  height: 162rpx;
  font-size: 28rpx;
  font-weight: 500;
  color: #c4c4c4;
  display: flex;
  align-items: center;
  justify-content: center;
}

