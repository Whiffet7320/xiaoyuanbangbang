page {
  width: 100%;
  height: 100%;
  background: #f7f8fa;
}
.index {
  width: 100%;
}
.index .TipsList {
  width: 710rpx;
  height: 88rpx;
  margin: 20rpx auto 0;
  background: #ffffff;
  border-radius: 10rpx;
  display: flex;
  align-items: center;
  justify-content: space-between;
}
.index .TipsList .Test {
  margin-left: 30rpx;
  font-size: 28rpx;
  font-weight: 500;
  color: #333333;
}
.index .TipsList .icon {
  margin-right: 34rpx;
  display: flex;
  align-items: center;
}
.index .TipsList .icon image {
  width: 12rpx;
  height: 22rpx;
}

