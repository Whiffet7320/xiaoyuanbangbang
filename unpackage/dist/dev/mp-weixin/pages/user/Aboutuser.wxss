page {
  width: 100%;
  height: 100%;
  background: #f7f8fa;
}
.index {
  width: 100%;
}
.index .About {
  width: 686rpx;
  height: 1426rpx;
  margin: 20rpx auto;
  background: #ffffff;
  border-radius: 10rpx 10rpx 0rpx 0rpx;
  display: flex;
  flex-direction: column;
}
.index .About .Title {
  width: 626rpx;
  height: 109rpx;
  margin: 0 auto;
  border-bottom: 2rpx dashed #e5e5e5;
  display: flex;
  align-items: center;
  justify-content: center;
  font-size: 32rpx;
  font-weight: 700;
  color: #000000;
}
.index .About .Conentn {
  margin: 28rpx 32rpx;
  margin-top: 29rpx;
  font-size: 28rpx;
  font-weight: 400;
  color: #000000;
}

