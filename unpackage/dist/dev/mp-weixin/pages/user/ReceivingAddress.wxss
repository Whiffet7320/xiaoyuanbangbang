@charset "UTF-8";
/**
 * 下方引入的为uView UI的集成样式文件，为scss预处理器，其中包含了一些"u-"开头的自定义变量
 * 使用的时候，请将下面的一行复制到您的uniapp项目根目录的uni.scss中即可
 * uView自定义的css类名和scss变量，均以"u-"开头，不会造成冲突，请放心使用 
 */
page {
  width: 100%;
  height: 100%;
  background-color: #F7F8FA;
}
.index {
  width: 100%;
}
.index .DizhiIcon {
  display: flex;
  flex-direction: column;
  align-items: center;
  margin: 400rpx auto 60rpx;
}
.index .DizhiIcon .icon {
  display: flex;
  align-items: center;
}
.index .DizhiIcon .icon image {
  width: 234rpx;
  height: 204rpx;
}
.index .DizhiIcon .Test {
  margin-top: 26rpx;
  font-size: 20rpx;
  font-weight: 500;
  color: #000000;
}
.index .AddRess {
  height: 252rpx;
  background: #ffffff;
  border: 2rpx solid #f4f4f4;
  overflow: hidden;
  border-radius: 20rpx;
  margin: 20rpx 24rpx;
  display: flex;
  align-items: center;
}
.index .AddRess .Bito {
  height: 100%;
  display: flex;
  flex-direction: column;
  justify-content: center;
  margin: 33rpx 28rpx 31rpx;
}
.index .AddRess .Bito .Hiurs {
  display: flex;
  align-items: center;
  justify-content: space-between;
}
.index .AddRess .Bito .Hiurs .NaPh {
  display: flex;
  align-items: center;
}
.index .AddRess .Bito .Hiurs .NaPh .Name {
  font-size: 28rpx;
  font-weight: 400;
  color: #000000;
}
.index .AddRess .Bito .Hiurs .NaPh .Phone {
  margin-left: 28rpx;
  font-size: 28rpx;
  font-weight: 400;
  color: #000000;
}
.index .AddRess .Bito .Hiurs .Defas {
  width: 58rpx;
  height: 28rpx;
  display: flex;
  align-items: center;
  justify-content: center;
}
.index .AddRess .Bito .Hiurs .Defas .DeTes {
  width: 58rpx;
  height: 28rpx;
  display: flex;
  align-items: center;
  justify-content: center;
  background-color: #FE694F;
  font-size: 20rpx;
  font-weight: 400;
  color: #ffffff;
}
.index .AddRess .Bito .DDress {
  margin: 33rpx 0 59rpx 0;
  width: 520rpx;
  height: 34rpx;
  font-size: 24rpx;
  font-weight: 400;
  color: #000000;
}
.index .AddRess .Bito .Defes {
  display: flex;
  align-items: center;
}
.index .AddRess .Bito .Defes .Pic {
  display: flex;
  align-items: center;
}
.index .AddRess .Bito .Defes .Pic image {
  width: 34rpx;
  height: 34rpx;
}
.index .AddRess .Bito .Defes .Tes {
  margin-left: 19rpx;
  font-size: 26rpx;
  font-weight: 400;
  color: #000000;
}
.index .AddRess .Delete {
  margin-left: 130rpx;
  width: 215rpx;
  height: 252rpx;
  border-radius: 0 20rpx 20rpx 0;
  background-color: #FE694F;
  display: flex;
  align-items: center;
  justify-content: center;
  font-size: 22rpx;
  font-weight: 400;
  color: #ffffff;
}
.index .GoAdds {
  width: 612rpx;
  height: 80rpx;
  margin: 136rpx auto 0;
  background: #fe694f;
  border-radius: 10rpx;
  display: flex;
  align-items: center;
  justify-content: center;
  font-size: 28rpx;
  font-weight: 700;
  color: #ffffff;
}

